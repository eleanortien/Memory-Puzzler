  - character: {fileID: 11400000, guid: b52d8c33c3f630d41a7d3fc4f4f8b6fb, type: 2}
    characterEmotion: 0
    hasImage: 0
    cutsceneImage: {fileID: 0}
    dialogueLines: 'P-please d-don''t. I-I want to do the trials with you... '
